{
  "name": "FinanceTracker Dev",
  "dockerComposeFile": [
    "../deploy/compose/compose.base.yml",
    "../deploy/compose/compose.local.yml"
  ],
  "service": "devtools",
  "workspaceFolder": "/workspaces/financetracker",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csharp",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ]
    }
  },
  "postCreateCommand": "dotnet --info && node --version && npm --version"
}
